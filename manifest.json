{
  "manifest_version": 2,

  "name": "Top Visited Websites",
  "description": "This extension shows your top 10 most visited websites for the past week.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://d3js.org/d3.v3.min.js",
    "history"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["d3.min.js"],
      "run_at" : "document_start"
    }
  ]
}
